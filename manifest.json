{
  "name": "Tabs Manager",
  "version": "1.0",
  "description": "Auto suspend and close",
  "permissions": ["activeTab", "declarativeContent", "storage", "tabs"],
  "options_page": "options/index.html",
  "background": {
    "scripts": ["common/functions.js", "background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "images/toggle16.png",
      "32": "images/toggle32.png",
      "48": "images/toggle32.png",
      "128": "images/toggle128.png"
    }
  },
  "icons": {
    "16": "images/toggle16.png",
    "32": "images/toggle32.png",
    "48": "images/toggle32.png",
    "128": "images/toggle128.png"
  },
  "manifest_version": 2
}
