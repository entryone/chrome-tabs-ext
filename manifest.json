{
  "name": "Tabs Killer",
  "version": "2.0",
  "description": "Auto kill your forbidden tabs",
  "permissions": ["activeTab", "declarativeContent", "storage", "tabs", "contextMenus"],
  "options_page": "options/dist/index.html",
  "background": {
    "service_worker": "./common/background.js"
  },
  "action": {
    "default_popup": "popup/dist/index.html",
    "default_icon": {
      "16": "images/kill.png",
      "32": "images/kill.png",
      "48": "images/kill_btn_48.png",
      "128": "images/kill_btn_128.png"
    }
  },
  "icons": {
    "16": "images/kill.png",
    "32": "images/kill.png",
    "48": "images/kill_btn_48.png",
    "128": "images/kill_btn_128.png"
  },
  "manifest_version": 3,
  "web_accessible_resources": [

  ]
}
