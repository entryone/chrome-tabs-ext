{
  "name": "Tabs Manager",
  "version": "1.0",
  "description": "Auto suspend and close",
  "permissions": ["activeTab", "declarativeContent", "storage", "tabs", "http://*/", "https://*/"],
  "options_page": "options/index.html",
  "background": {
    "scripts": ["common/functions.js", "background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "images/popup_btn_16.png",
      "32": "images/popup_btn_32.png",
      "48": "images/popup_btn_48.png",
      "128": "images/popup_btn_128.png"
    }
  },
  "icons": {
    "16": "images/popup_btn_16.png",
    "32": "images/popup_btn_32.png",
    "48": "images/popup_btn_48.png",
    "128": "images/popup_btn_128.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": ["images/stay-focused.jpg"]
}
