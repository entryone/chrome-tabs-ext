{
  "name": "Tabs Killer",
  "version": "2.0",
  "description": "Auto kill your forbidden tabs",
  "permissions": ["activeTab", "declarativeContent", "storage", "tabs", "http://*/", "https://*/", "contextMenus"],
  "options_page": "options/dist/index.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup/dist/index.html",
    "default_icon": {
      "16": "images/popup_btn_16.png",
      "32": "images/popup_btn_32.png",
      "48": "images/popup_btn_48.png",
      "128": "images/popup_btn_128.png"
    }
  },
  "icons": {
    "16": "images/popup_btn_16.png",
    "32": "images/popup_btn_32.png",
    "48": "images/popup_btn_48.png",
    "128": "images/popup_btn_128.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": ["images/stay-focused.jpg"]
}
